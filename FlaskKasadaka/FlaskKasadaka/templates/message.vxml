{% extends "vxmlLayout.vxml" %}
{#
plays back a message, then sends the user to the redirect
#}

{% block content %}
<form>
<block>
<prompt>
{% for messageAudio in messages %}
<audio src="{{messageAudio}}"/>
{% endfor %}
</prompt>
<goto next="{{ redirect }}"/>
</block>


</form>
{% if error is defined %}
<!-- Error: {{error}} -->
{% endif %}
{% endblock %}
