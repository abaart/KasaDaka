@prefix    rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix     rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix   speakle: <http://purl.org/collections/w4ra/speakle/> .
@prefix      fb: <http://example.org/foroba-blon/> .
@prefix      audio: <http://127.0.0.1/audio/>.
@prefix   lvont: <http://lexvo.org/ontology#>.
@prefix   lexvoid: <http://lexvo.org/id/iso639-3/>.


# Setup data for foroba-blon use-case
# messages
	
fb:message_1
	a fb:message ;
	fb:contact_tel fb:andre_baart	;
	fb:date "2016-06-01 12:00:00" ;
	speakle:voicelabel_en <http://localhost/audio/stupidmessage.wav> .
	

# Languages

speakle:voicelabel_en
	a rdf:Property ;
	lvont:language lexvoid:eng; 
	rdfs:label "English voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_fr
	a rdf:Property ;
	lvont:language lexvoid:fra;
	rdfs:label "French voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_nl
	a rdf:Property ;
	lvont:language lexvoid:nld;
	rdfs:label "Dutch voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel
	a rdf:Property ;
	rdfs:label "voicelabel".

# Properties

fb:contact_fname
	a rdf:Property;
	rdfs:label "Contact First Name".

fb:contact_lname
	a rdf:Property;
	rdfs:label "Contact Last Name".

fb:contact_tel
	a rdf:Property;
	rdfs:label "Contact Telephone Number".

fb:preferred_language
	a rdf:Property;
	rdfs:label "Preferred Language".

fb:user
	a rdf:Property;
	rdfs:label "User".

fb:message
	a rdf:Property;
	rdfs:label "Message".

fb:date
	a rdf:Property;
	rdfs:label "Date".
	
