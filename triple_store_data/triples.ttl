@prefix    rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix     rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix   speakle: <http://purl.org/collections/w4ra/speakle/> .
@prefix      cv: <http://example.org/chickenvaccinationsapp/> .
@prefix      audio: <http://127.0.0.1/audio/>.
@prefix   lvont: <http://lexvo.org/ontology#>.
@prefix   lexvoid: <http://lexvo.org/id/iso639-3/>.


# Setup data for chicken vaccination use-case


# Users

cv:andre_baart
	a cv:user ;
	speakle:voicelabel_nl audio:andre_baart_nl.wav;
	speakle:voicelabel_en audio:andre_baart_en.wav;
	cv:contact_fname "Andre" ;
	cv:contact_lname "Baart" ;
	cv:contact_tel "+31653105713" ;
	cv:preferred_language speakle:voicelabel_en .

cv:victor_de_boer
	a cv:user ;
	speakle:voicelabel_nl audio:victor_de_boer_nl.wav;
	speakle:voicelabel_en audio:victor_de_boer_en.wav;
	cv:contact_fname "Victor" ;
	cv:contact_lname "de Boer" ;
	cv:contact_tel "+31612345678" ;
	cv:preferred_language speakle:voicelabel_nl .


	
# Diseases
	
cv:newcastle
	a cv:disease ;
	rdfs:label "Newcastle's disease"@en ;
	cv:occurs_in cv:chicken_batch ;
	speakle:voicelabel_nl audio:newcastle_nl.wav ;
	speakle:voicelabel_en audio:newcastle_en.wav .
	


# Chicken batches

cv:chicken_batch_1
	a cv:chicken_batch ;
	cv:birth_date "2012-08-11" ;
	cv:owned_by cv:andre_baart ;
	speakle:voicelabel_nl audio:chicken_batch1_nl.wav ;
	speakle:voicelabel_en audio:chicken_batch1_en.wav .	
	
cv:chicken_batch_2
	a cv:chicken_batch ;
	cv:birth_date "2010-08-11" ;
	cv:owned_by cv:andre_baart ;
	speakle:voicelabel_nl audio:chicken_batch1_nl.wav ;
	speakle:voicelabel_en audio:chicken_batch1_en.wav .
	
# Vaccinations

cv:newcastle_vac_1
	a cv:vaccination ;
	cv:treats cv:newcastle ;
	cv:days_after_birth "1" ;
	cv:description "description goes here" ;
	speakle:voicelabel_nl audio:newcastle_vac1_nl.wav ;
	speakle:voicelabel_en audio:newcastle_vac1_en.wav .	
	
cv:newcastle_vac_2
	a cv:vaccination ;
	cv:treats cv:newcastle ;
	cv:days_after_birth "5" ;
	cv:description "vaccination 2" ;
	speakle:voicelabel_nl audio:newcastle_vac1_nl.wav ;
	speakle:voicelabel_en audio:newcastle_vac1_en.wav .


# Languages

speakle:voicelabel_en
	a rdf:Property ;
	lvont:language lexvoid:eng; 
	rdfs:label "English voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_fr
	a rdf:Property ;
	lvont:language lexvoid:fra;
	rdfs:label "French voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_nl
	a rdf:Property ;
	lvont:language lexvoid:nld;
	rdfs:label "Dutch voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel
	a rdf:Property ;
	rdfs:label "voicelabel".

# Properties

cv:contact_fname
	a rdf:Property;
	rdfs:label "Contact First Name".

cv:contact_lname
	a rdf:Property;
	rdfs:label "Contact Last Name".

cv:contact_tel
	a rdf:Property;
	rdfs:label "Contact Telephone Number".

cv:preferred_language
	a rdf:Property;
	rdfs:label "Preferred Language".

cv:user
	a rdf:Property;
	rdfs:label "User".

cv:disease
	a rdf:Property;
	rdfs:label "Disease".

cv:chicken_batch
	a rdf:Property;
	rdfs:label "Chicken Batch".

cv:birth_date
	a rdf:Property;
	rdfs:label "Birth Date".

cv:owned_by
	a rdf:Property;
	rdfs:label "Owned By".

cv:vaccination
	a rdf:Property;
	rdfs:label "Vaccination".

cv:treats
	a rdf:Property;
	rdfs:label "Treats".

cv:days_after_birth
	a rdf:Property;
	rdfs:label "Days after birth".

cv:description
	a rdf:Property;
	rdfs:label "Description".

cv:occurs_in
	a rdf:Property;
	rdfs:label "Occurs in".