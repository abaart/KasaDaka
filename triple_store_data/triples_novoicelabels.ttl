@prefix    rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix     rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix   speakle: <http://purl.org/collections/w4ra/speakle/> .
@prefix      cv: <http://example.org/foroba-blon/> .
@prefix      audio: <http://127.0.0.1/audio/>.
@prefix   lvont: <http://lexvo.org/ontology#>.
@prefix   lexvoid: <http://lexvo.org/id/iso639-3/>.


# Setup data for chicken vaccination use-case


# Users

cv:andre_baart
	a cv:user ;
	cv:contact_fname "Andre" ;
	cv:contact_lname "Baart" ;
	cv:contact_tel "31653105713" ;
	cv:preferred_language speakle:voicelabel_en .

cv:victor_de_boer
	a cv:user ;
	cv:contact_fname "Victor" ;
	cv:contact_lname "de Boer" ;
	cv:contact_tel "316123456" ;
	cv:preferred_language speakle:voicelabel_nl .
	
# messages
	
cv:message_1
	a cv:message ;
	cv:left_by_user cv:andre_baart	.
	

# Languages

speakle:voicelabel_en
	a rdf:Property ;
	lvont:language lexvoid:eng; 
	rdfs:label "English voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_fr
	a rdf:Property ;
	lvont:language lexvoid:fra;
	rdfs:label "French voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel_nl
	a rdf:Property ;
	lvont:language lexvoid:nld;
	rdfs:label "Dutch voicelabel"@en;
	rdfs:subPropertyOf speakle:voicelabel .
	
speakle:voicelabel
	a rdf:Property ;
	rdfs:label "voicelabel".

# Properties

cv:contact_fname
	a rdf:Property;
	rdfs:label "Contact First Name".

cv:contact_lname
	a rdf:Property;
	rdfs:label "Contact Last Name".

cv:contact_tel
	a rdf:Property;
	rdfs:label "Contact Telephone Number".

cv:preferred_language
	a rdf:Property;
	rdfs:label "Preferred Language".

cv:user
	a rdf:Property;
	rdfs:label "User".

cv:message
	a rdf:Property;
	rdfs:label "Message".

cv:left_by_user
	a rdf:Property;
	rdfs:label "Left by user".
	
